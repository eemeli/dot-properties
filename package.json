{
  "name": "dot-properties",
  "version": "1.1.0",
  "license": "MIT",
  "author": "Eemeli Aro <eemeli@gmail.com>",
  "repository": "github:eemeli/dot-properties",
  "description": "Parse & stringify .properties files",
  "keywords": [
    "properties",
    "java",
    "resource",
    "bundle",
    "i18n"
  ],
  "types": "./lib/index.d.ts",
  "main": "./lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "test": "jest"
  },
  "jest": {
    "collectCoverage": true,
    "testMatch": [
      "**/tests/*.js"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "devDependencies": {
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  }
}
